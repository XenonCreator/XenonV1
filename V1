local function ZUQYV_fake_script() -- ComingSoon.Script 
	local script = Instance.new('Script', ComingSoon)

	while true do
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "C"
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "CO"
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "COM"
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "COMI"
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "COMIN"
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "COMING"
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "COMING"
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "COMING "
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "COMING S"
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "COMING SO"
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "COMING SOO"
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "COMING SOON"
		wait(0.8)
		script.Parent.Parent.ComingSoon.Text = "COMING SOON!"
		
	end
	print("Hello world!")
	
end
coroutine.wrap(ZUQYV_fake_script)()
